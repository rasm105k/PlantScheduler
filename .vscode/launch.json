    "version": "0.2.0",
    "configurations": [
        {
            "name": "C# Api Debug",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}/PLantScheduler.BackEnd/PlantScheduler.Api.c#/PlantScheduler.Api.csproj"
        },
        {
            "name": "Node API Debug",
            "type": "node",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/PlantScheduler.BackEnd/PlantScheduler.Api.js/apis/plantController.js"
        },
        {
            "name": "Launch Frontend (Five Server)",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:5500//PlantScheduler.FrontEnd/index.html",
            "webRoot": "${workspaceFolder}/PlantScheduler.FrontEnd",
        }
    ]
}